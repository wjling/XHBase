# Uncomment the next line to define a global platform for your project
platform :ios, '9.0'

workspace 'XHBase.xcworkspace'

target 'XHBase-Example' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Define the target's project, or Pod will use the default project XHBase
  project 'XHBase-Example/XHBase-Example.xcodeproj'
  # Pods for XHBase
  pod 'XHBase', :path => './'

end
